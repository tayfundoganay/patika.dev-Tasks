import java.util.Scanner;

public class manavKasaProgrami {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        double fiyatArmut = 2.14, fiyatElma = 3.67, fiyatDomates = 1.11, fiyatMuz = 0.95, fiyatPatlican = 5.00;
        int kgArmut, kgElma, kgDomates, kgMuz, kgPatlican;
        System.out.print("Armut Miktari(KG) = ");
        kgArmut = scanner.nextInt();
        System.out.print("Elma Miktari(KG) = ");
        kgElma = scanner.nextInt();
        System.out.print("Domates Miktari(KG) = ");
        kgDomates = scanner.nextInt();
        System.out.print("Muz Miktari(KG) = ");
        kgMuz = scanner.nextInt();
        System.out.print("Patlican Miktari(KG) = ");
        kgPatlican = scanner.nextInt();

        double ucret = ((fiyatArmut * kgArmut) + (fiyatElma * kgElma) + (fiyatDomates * kgDomates) + (fiyatMuz * kgMuz) + (fiyatPatlican * kgPatlican));
        System.out.println("Toplam Ucret = " + ucret);
    }
}
